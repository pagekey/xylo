file_replacers:
  - name: Cargo.toml
    type: toml
    key: package.version
  - name: package.json
    type: json
    key: version
integrations:
  create_release:
    github:
      project: "pagekey/xylo"
      token_variable: "SEMVER_TOKEN"
      title_format: "v%M.%m.%p"
      body: "Release generated by PageKey Semver."
