file_replacers:
  - type: toml
    name: pyproject.toml
    key: tool.poetry.version
    format: "%M.%m.%p"
integrations:
  create_release:
    github:
      project: "pagekey/xylo"
      token_variable: "SEMVER_TOKEN"
      title_format: "v%M.%m.%p"
      body: "Release generated by PageKey Semver."
